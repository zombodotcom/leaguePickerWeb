{
  "manifest_version": 3,
  "name": "League Arena Champion Tracker",
  "version": "1.0.0",
  "description": "Track your League of Legends Arena Champion challenge progress by connecting directly to your local League client",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:*/*",
    "https://127.0.0.1:*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "League Arena Champion Tracker"
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  }
}
